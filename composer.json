        "ratchet/pawl": "^0.4.1",
        "react/socket": "^1.0 || ^0.8 || ^0.7",
        "ratchet/rfc6455": "^0.3",
        "aws/aws-sdk-php": "^3.231"
    },
    "require-dev": {
        "phpunit/phpunit": "~6",
        "codacy/coverage": "dev-master"
    },
    "config": {
    	"optimize-autoloader": true,
        "minimum-stability": "stable"
    },
    "autoload": {
